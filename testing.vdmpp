class Testing

instance variables
    public m : map nat1 to nat1 := { 1 |-> 2, 2 |-> 3, 3 |-> 4};

operations
    public MapTest : nat1 ==> nat1
    MapTest(x) == return m(x);

functions
    public SetTest : set of nat1 * nat1 -> [nat1]
    SetTest(s, x) == if card s = 1 and x in set s then nil else let y in set s \ {x} in y;
    -- SetTest(s, x) == let y in set s \ {x} in if y <> nil then y else -1;

end Testing